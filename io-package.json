{
	"common": {
		"name": "mihome-plug",
		"version": "0.1.1",
		"news": {
			"0.1.1": {
				"en": "Inital version",
				"de": "Erste Version",
				"ru": "Первая версия"
			}
		},
		"title": "mihome-plug Adapter",
		"desc": "This adapter allows control Xiaomi Plug (wifi) ",
		"authors": [
			"bluefox <dogafox@gmail.com>"
		],
		"license": "MIT",
		"platform": "Javascript/Node.js",
		"mode": "daemon",
		"loglevel": "info",
		"icon": "mihome-plug.png",
		"webExtension": "lib/mihome-plug.js",
		"readme": "https://github.com/ioBroker/ioBroker.mihome-plug/blob/master/README.md",
		"keywords": [
			"url",
			"html",
			"file",
			"mihome-plug"
		],
		"extIcon": "https://raw.githubusercontent.com/ioBroker/ioBroker.mihome-plug/master/admin/mihome-plug.png",
		"type": "hardware",
		"config": {
			"minWidth": 800,
			"width ": 1224
		}
	},
	"native": {
		"ip": "0.0.0.0",
		"port": 54321,
		"ownPort": 53423,
		"pingInterval": 20000,
		"token": "FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF"
	},
	"instanceObjects": [
		{
			"_id": "info",
			"type": "channel",
			"common": {
				"name": "Information"
			},
			"native": {}
		},
		{
			"_id": "info.connection",
			"type": "state",
			"common": {
				"role": "indicator.connected",
				"name": "If connected to Plug",
				"type": "boolean",
				"read": true
			},
			"native": {}
		},
		{
			"_id": "power",
			"type": "state",
			"common": {
				"name": "Plug power",
				"type": "boolean",
				"read": true,
				"write": true,
				"role": "state"
			},
			"native": {}
		},
		{
			"_id": "temperature",
			"type": "state",
			"common": {
				"name": "Plug temperature",
				"type": "number",
				"read": true,
				"write": false,
				"unit": "°C",
				"role": "value.temperature"
			},
			"native": {}
		},
		{
			"_id": "wifi_led",
			"type": "state",
			"common": {
				"name": "Plug wifi LED",
				"type": "boolean",
				"read": true,
				"write": true,
				"role": "state"
			},
			"native": {}
		}
	]
}
